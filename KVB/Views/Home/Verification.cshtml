@{
    Layout = "_Layout";
}
<div class="content-wrapper">
    <div class="content-header">

        <div class="container-fluid">

            <div class="card mb-2">

                <div class="card-header">

                    <h1 class="m-0">Verification Report</h1>

                </div><!-- /.col -->
                <form method="post">
                <div class="datePickers d-flex justify-content-start mt-3">
                    &nbsp; 
                    <div class="startDate">
                        Start Date
                           <input name="inputStartDate" type="date" asp-format="{0:yyyy-MM-dd}" required />
                    </div>
                    &nbsp&nbsp 
                    <div class="endDate ">
                        End Date
                        <input name="inputEndDate" type="date" asp-format="{0:yyyy-MM-dd}" required />
                    </div>
                    &nbsp;&nbsp;<div>
                            <button formaction="Verification">SUBMIT</button>
                    </div>
                </div>
                 </form>
                <table class="table  table-striped table-bordered" id="dtable">
                    <thead >
                        <tr>
                            <th> date</th>
                            &nbsp;
                            <th>customerId</th>
                            <th>uniqueId </th>
                            <th>Verification1 </th>
                            <th>captchaId</th>
                            <th>captchaReturn</th>
                            <th>digit </th>
                            <th>Verification2 </th>

                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var vr in Model) // Loop through each Employee object in the model
                        {
                            <tr>
                                <td>@vr.date</td>
                                <td>@vr.customerId</td>
                                <td>@vr.uniqueId</td>
                                <td>@vr.Verification1</td>
                                <td>@vr.captchaId</td>
                                <td>@vr.captchaReturn</td>
                                <td>@vr.digit</td>
                                <td>@vr.Verification2</td>

                            </tr>
                        }
                    </tbody>
                </table>


            </div><!-- /.row -->

        </div><!-- /.container-fluid -->

    </div>
</div>
<script>
    $(document).ready(function () {
       // $('#dtable').DataTable();
        let table = new DataTable('#dtable');
    });
</script>
